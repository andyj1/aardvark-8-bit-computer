#! /c/iverilog/bin/vvp
:ivl_version "0.9.6 " "(v0_9_6)";
:vpi_time_precision - 9;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00892F50 .scope module, "tb" "tb" 2 8;
 .timescale 0 0;
v0089DF30_0 .var "data1", 4 0;
v0089DF88_0 .net "result", 7 0, v00892B98_0; 1 drivers
S_00892AB8 .scope module, "signext" "signext5" 2 21, 3 10, S_00892F50;
 .timescale -9 -9;
v00892B40_0 .net "in", 4 0, v0089DF30_0; 1 drivers
v00892B98_0 .var "out", 7 0;
v0089DED8_0 .var "sign", 2 0;
E_010C2038 .event edge, v00892B98_0;
    .scope S_00892AB8;
T_0 ;
    %wait E_010C2038;
    %movi 8, 16, 5;
    %load/v 13, v00892B40_0, 5;
    %cmp/u 8, 13, 5;
    %or 5, 4, 1;
    %jmp/0xz  T_0.0, 5;
    %set/v v0089DED8_0, 1, 3;
T_0.0 ;
    %load/v 8, v00892B40_0, 5;
   %cmpi/u 8, 16, 5;
    %jmp/0xz  T_0.2, 5;
    %set/v v0089DED8_0, 0, 3;
T_0.2 ;
    %load/v 8, v00892B40_0, 5;
    %load/v 13, v0089DED8_0, 3;
    %set/v v00892B98_0, 8, 8;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_00892F50;
T_1 ;
    %vpi_call 2 26 "$display", "<<Starting the Simulation>>";
    %movi 8, 21, 5;
    %set/v v0089DF30_0, 8, 5;
    %vpi_call 2 28 "$display", "data1: %b result: %b", v0089DF30_0, v0089DF88_0;
    %vpi_call 2 29 "$display", "data1: %d result: %d", v0089DF30_0, v0089DF88_0;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb.v";
    "signext5.v";
